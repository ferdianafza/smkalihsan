  <div id="fh5co-blog">
    <div class="container">
      <div class="row animate-box">
        <div class="col-md-8 col-md-offset-2 text-center fh5co-heading">
          <h2>Blog</h2>
          <p>Dignissimos asperiores vitae velit veniam totam fuga molestias accusamus alias autem provident. Odit ab aliquam dolor eius.</p>
        </div>
      </div>
      <div class="row">
        <% @blogs.each do |blog| %>
          <div class="col-lg-4 col-md-4">
            <div class="fh5co-blog animate-box">
              <a href="/blogs/<%= blog.id %>" class="blog-img-holder" style="background-image: url(<%= url_for(blog.banner) %>);"></a>
              <div class="blog-text">
                <h3><a id="lsblog" href="/blogs/<%= blog.id %>"><%= blog.judul %></a></h3>
                <span class="posted_on"><%= blog.created_at.to_date %></span>
                <span class="comment"><a href="#"><i class="fas fa-star" style="color: yellow;"></i></a></span>
                <p><%= truncate(strip_tags(blog.rich_text_content.to_s), length: 150) %>.</p>
              </div>
            </div>
          </div>
        <% end %>
      </div>
    </div>
  </div>


